	-- Navigation icons (testing with actual Roblox image IDs)
	swords = "rbxassetid://7733779610",     -- Test with known working ID
	target = "rbxassetid://7734042071",
	gun = "rbxassetid://7743871002",
	users = "rbxassetid://6031097225",
	knife = "rbxassetid://7733779610",
	globe = "rbxassetid://7734042071",
	user = "rbxassetid://7743871002",
	code = "rbxassetid://6031097225",
	settings = "rbxassetid://7733779610",
	
	-- Add more Lucide icons here as needed
	shield = "rbxassetid://7734042071",
	eye = "rbxassetid://7743871002",
	zap = "rbxassetid://6031097225",
}

-- Create ScreenGui
