mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0, 800, 0, 550)
mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.BackgroundTransparency = 1
mainFrame.BorderSizePixel = 0
mainFrame.ClipsDescendants = true
mainFrame.Parent = screenGui

-- Corner rounding
local mainCorner = Instance.new("UICorner")
mainCorner.CornerRadius = UDim.new(0, 12)
mainCorner.Parent = mainFrame

-- ========================================
-- LEFT SIDEBAR
-- ========================================

local sidebar = Instance.new("Frame")
sidebar.Name = "Sidebar"
sidebar.Size = UDim2.new(0, 200, 1, 0)
sidebar.Position = UDim2.new(0, 0, 0, 0)
sidebar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
sidebar.BorderSizePixel = 0
sidebar.Parent = mainFrame

local sidebarCorner = Instance.new("UICorner")
sidebarCorner.CornerRadius = UDim.new(0, 12)
sidebarCorner.Parent = sidebar

-- Animated red gradient background
local sidebarGradient = Instance.new("UIGradient")
sidebarGradient.Color = ColorSequence.new({
	ColorSequenceKeypoint.new(0, Color3.fromRGB(35, 10, 10)),
	ColorSequenceKeypoint.new(0.5, Color3.fromRGB(55, 15, 15)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(75, 20, 20))
})
sidebarGradient.Rotation = 0
sidebarGradient.Parent = sidebar

-- Animate gradient rotation
task.spawn(function()
	local gradientRotate = TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1)
	TweenService:Create(sidebarGradient, gradientRotate, {Rotation = 360}):Play()
end)

